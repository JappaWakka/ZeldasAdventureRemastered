	"Subtitles": { //Some of the Dutch localizations have more lines than English does because some of the lines became too long.
		"Cutscenes": {
			"Intro": {
				"Line01": "Gaspra: Er komt van sterrenkijken niets meer vannacht.",
				"Line02": "Ah, de Rol van Shurmak...",
				"Line03": "brenger van droevig nieuws al die jaren geleden.",
				"Line04": "En zo gebeurde het dat Ganon...",
				"Line05": "Heer van de Duisternis, erin slaagde Tolemac over te nemen.",
				"Line06": "Hij stal de kostbare Hemelse Tekens en nam Link gevangen!",
				"Line07": "Er zou een moedige krijger gevonden moeten worden",
				"Line08": "om de strijd aan te gaan met dat kwaadaardige monster.",
				"Line09": "En zo vond ik deze kampioen van kracht en moed.",
				"Line10": "U bent het, Prinses Zelda. Met deze magische ketting, ga voort...",
				"Line11": "en met elk Teken dat u vindt zult u ook meer kennis opdoen.",
				"Line12": "Vind uw magische toverstaf, gebruik hem verstandig",
				"Line13": "en luister naar de bondgenoten die u tegenkomt tijdens uw reis.",
				"Line14": "Ga nu, mijn prinses. Haal de Hemelse Tekens terug en red Link."
			},
			"Tutorial": {
				"Line01": "Welkom bij Zelda's Adventure, een actie-avonturen rollenspel.",
				"Line02": "Als speler neem je de persoonlijkheid aan van onze heldin Prinses Zelda.",
				"Line03": "Om het spel succesvol te spelen, moet je het grote land Tolemac verkennen.",
				"Line04": "Om Zelda's beweging door Tolemac te besturen,",
				"Line05": "beweeg je de controller naar links, rechts, omhoog, omlaag en diagonaal.",
				"Line06": "De hofastronoom Gaspra zal het verhaal van Zelda introduceren",
				"Line07": "en beschrijven hoe haar zoektocht tot stand is gekomen.",
				"Line08": "Hij zal ook telkens weer verschijnen",
				"Line09": "als Zelda met succes een tempel in de onderwereld heeft voltooid",
				"Line10": "en een van de zeven kostbare Hemelse Tekens heeft verzameld.",
				"Line11": "Een andere mentor en gids voor Zelda ben ik, Shurmak.",
				"Line12": "Gedurende het spel help ik Zelda te oriënteren en haar te begeleiden.",
				"Line13": "Tijdens haar verkenningstocht",
				"Line14": "zal Zelda veel vijanden tegenkomen die dit land bewonen.",
				"Line15": "Ze moet ze verslaan door vaardigheid en snelheid te gebruiken.",
				"Line16": "Zelda valt ze aan met haar toverstaf.",
				"Line17": "Je gebruikt de toverstaf door op Actieknop 1 te drukken.",
				"Line18": "Als Zelda succesvol is in het verslaan van haar vijanden, wordt ze beloond.",
				"Line19": "Ze laten willekeurig Robijnen en harten vallen",
				"Line20": "die respectievelijk haar valuta en levenseenheden verhogen.",
				"Line21": "Robijnen, de valuta van Tolemac,",
				"Line22": "kunnen worden gebruikt om nuttige voorwerpen te kopen",
				"Line23": "en worden aangegeven in de linkerbovenhoek van het scherm.",
				"Line24": "Harten zijn de levenseenheden die Zelda kracht geven",
				"Line25": "om te vechten tijdens het spel.",
				"Line26": "Ze worden weergegeven in de rechterbovenhoek van het speelscherm.",
				"Line27": "Om een voorwerp of een wapen te selecteren in het inventarisscherm",
				"Line28": "druk je op Actieknop 2.",
				"Line29": "Eenmaal in het inventarisscherm,",
				"Line30": "heb je toegang tot nuttige voorwerpen en wapens",
				"Line31": "door een voorwerp te selecteren met je cursor en op Actieknop 1 te drukken.",
				"Line32": "Schatten en wapens kun je scrollen door op de linker vensterknop te drukken",
				"Line33": "om naar links te gaan",
				"Line34": "en door op de rechter vensterknop te drukken om naar rechts te gaan.",
				"Line35": "Het wapen dat in gebruik is, verschijnt in het venster rechtsonder",
				"Line36": "in het inventarisatiescherm.",
				"Line37": "Als je een nuttig voorwerp selecteert uit de selectie schatten,",
				"Line38": "verschijnt het voorwerp in hetzelfde venster waar de wapenselectie verscheen.",
				"Line39": "Zelda kan maar één voorwerp of wapen tegelijk gebruiken.",
				"Line40": "Tijdens Zelda's reizen zal ze veel gesloten deuren tegenkomen.",
				"Line41": "Ze moet een sleutel in haar bezit hebben om door deze gesloten ingangen te kunnen.",
				"Line42": "Het aantal sleutels in Zelda's bezit verschijnt in het kleine venster",
				"Line43": "rechtsboven in het inventarisscherm.",
				"Line44": "Om te helpen met navigeren door Tolemac,",
				"Line45": "is een kaart beschikbaar door je joystick naar de Kaart-knop te bewegen",
				"Line46": "en dan op Actieknop 1 te drukken.",
				"Line47": "Er zijn twee soorten kaarten:",
				"Line48": "Eén voor de bovenwereld en één voor elk van de onderwerelden.",
				"Line49": "De kaart voor de bovenwereld laat zien waar je bent en waar je naartoe hebt gereisd.",
				"Line50": "De kaarten voor elk van de onderwerelden laten zien waar je bent,",
				"Line51": "waar je hebt gereisd en waar je heen moet om je zoektocht te voltooien.",
				"Line52": "Druk op Actieknop 2 om terug te keren naar het spel.",
				"Line53": "Als je klaar bent om het spel te verlaten",
				"Line54": "ga je met je cursor naar de knop Afsluiten en druk je op Actieknop 1.",
				"Line55": "Je komt dan in het scherm voor opslaan, doorgaan en afsluiten.",
				"Line56": "Om een van deze knoppen te activeren,",
				"Line57": "moet je de cursor naar de knop verplaatsen en op Actieknop 1 drukken.",
				"Line58": "Als je opslaat en stopt, kom je bij het speler selectiescherm",
				"Line59": "waar je de optie hebt om opnieuw te spelen of het spel te verlaten.",
				"Line60": "Als je terugkomt om opnieuw te spelen,",
				"Line61": "kun je opnieuw beginnen waar je gebleven was door je naam te selecteren.",
				"Line62": "Je keert dan terug naar het eerste scherm van de gameplay",
				"Line63": "met alle items en wapens nog in de inventaris.",
				"Line64": "Als je stopt zonder op te slaan, kom je bij het speler selectiescherm,",
				"Line65": "waar je de optie hebt om opnieuw te spelen of het spel af te sluiten.",
				"Line66": "Als je terugkeert naar de gameplay,",
				"Line67": "kun je niet terugkeren naar je vorige spel.",
				"Line68": "Je status zal echter wel gelijk zijn aan het laatste spel dat je hebt opgeslagen.",
				"Line69": "Als je \"Terug naar Spel\" kiest,",
				"Line70": "kom je bij het inventarisscherm waar je op Actieknop 2 drukt,",
				"Line71": "wat je vervolgens terugbrengt naar de gameplay.",
				"Line72": "Voor elk spel moet je een naam kiezen voor je held of heldin.",
				"Line73": "Je kunt de Creëer-knop kiezen door de cursor op de knop te zetten",
				"Line74": "en op Actieknop 1 te drukken.",
				"Line75": "Dit brengt je naar het naaminvoerscherm,",
				"Line76": "waar je je naam intypt op het toetsenbord",
				"Line77": "door de gewenste letters te selecteren met Actieknop 1",
				"Line78": "en vervolgens je cursor op de knop Klaar te zetten",
				"Line79": "en nogmaals op Actieknop 1 te drukken.",
				"Line80": "Dit brengt je terug naar het speler selectiescherm.",
				"Line81": "Hier druk je op Speel door de Speel-knop te selecteren",
				"Line82": "en op Actieknop 1 te drukken.",
				"Line83": "Je begint dan met het spel.",
				"Line84": "Om de naam van een speler te wissen, plaats je de cursor op de Wis-knop",
				"Line85": "en hou je drie seconden lang de Actieknop 1 ingedrukt.",
				"Line86": "Om het spel te verlaten, plaats je de cursor op de Stop-knop",
				"Line87": "en druk je op Actieknop 1.",
				"Line88": "Dat is alles wat ik voor nu zal onthullen.",
				"Line89": "De rest is aan jou om te ontdekken."
			},
			"ShrineOfEarth": {
				"Line01": "Gaspra: Gefeliciteerd, Zelda. Je hebt gezegevierd.",
				"Line02": "Haast je nu. Je moet naar een festivalplaats reizen.",
				"Line03": "Het gerucht gaat dat het in het grote bos in het noorden te vinden is.",
				"Line04": "En onthoud: niet alles is wat het lijkt."
			}
		},
		"Dialogue": {
			"VisionHenge": {
				"Shurmak": {
					"Introduction": {
						"Line01": "Shurmak: Ik ben Shurmak, jouw gids.",
						"Line02": "Ik ken je al sinds je nog maar een kind was.",
						"Line03": "Je hebt een moeilijke taak gekregen.",
						"Line04": "Je zult ver moeten reizen en grote gevaren moeten trotseren.",
						"Line05": "Blijf kalm en gebruik je kennis en kracht om je vijanden te verslaan.",
						"Line06": "Onthoud wat je leert tijdens je reis en geef niet op.",
						"Line07": "Ga nu naar degene die een bed heeft, maar nooit slaapt",
						"Line08": "waarbij een rode wilg waakt",
						"Line09": "en naar de mond die nooit praat."
					},
					"DoNotRoam": {
						"Line01": "Shurmak: Zelda! Zelda! Zwerf niet onbeschermd rond.",
						"Line02": "Kijk in de buurt of er iets is dat je kan helpen."
					},
					"DoNotBeHasty": {
						"Line01": "Shurmak: Zelda, wees niet zo haastig!",
						"Line02": "Er is hier iets in de buurt dat je moet hebben.",
						"Line03": "Verkenning is de sleutel tot jouw succes."
					}
				}
			},
			"PlainOfAndor": {
				"BeggarWoman": {
					"BeforeGiveRupees": {
						"Line01": "Bedelaar: Oh, schone prinses,",
						"Line02": "heb meelij met een arm wezen, verbannen uit Great Wimbich in het noorden!",
						"Line03": "Ik vraag alleen genoeg voor één maaltijd voor mijn kinderen,",
						"Line04": "die honger lijden en kou hebben van de wind."
					},
					"AfterGiveRupees": {
						"Line01": "Bedelaar: U zijt duizendmaal gezegend,",
						"Line02": "duizendmaal, schone prinses!"
					},
					"WhenAttacked": {
						"Line01": "Bedelaar: U moet liefdadigheid leren voordat u een echte prinses kunt worden!",
						"Line02": "Moge u de rode wilg bij de grot nooit zien!"
					}
				},
				"GlebbTheThirsty": {
					"BeforeFill": {
						"Line01": "Glebb de Dorstige: Jonge prinses, kom alstublieft bij me.",
						"Line02": "Een reiziger met goede bedoelingen heeft me eindelijk gevonden!",
						"Line03": "Ik ben ingestort van de dorst en droogte."
					},
					"AfterFill": {
						"Line01": "Glebb de Dorstige: U zijt gezegend, Prinses!",
						"Line02": "U heeft het leven van een vermoeide oude vrouw gered!",
						"Line03": "Ik bied u deze schatten aan."
					},
					"WhenAttacked": {
						"Line01": "Glebb de Dorstige: Jij monster!",
						"Line02": "Hoe durft u een zwakke, oude vrouw aan te vallen?!",
						"Line03": "Hiervoor verliest u een leven!"
					}
				},
				"MobilinsHeadInn": {
					"LotharTheInnKeeper": {
						"BeforeBuy": {
							"Line01": "Lothar de Herbergier: Welkom bij Herberg de Kop van de Mobilin, mijn prinses!",
							"Line02": "U ziet er uitgedroogd uit, neem een kopje Andor cider van het huis.",
							"Line03": "Ik heb een Kalmte-spreuk te koop, maar hij kost u 100 Robijnen."
						},
						"AfterBuy": {
							"Line01": "Lothar de Herbergier: Welkom bij Herberg de Kop van de Mobilin, mijn prinses!",
							"Line02": "U ziet er uitgedroogd uit, neem een kopje Andor cider van het huis."
						},
						"PurchaseCalmSpell": {
							"Line01": "Lothar de Herbergier: Dat is wijsheid die uw jaren te boven gaat.",
							"Line02": "De Kalmte-spreuk zal u helpen wanneer u hem het meest nodig heeft, maak er goed gebruik van.",
							"Line03": "En let goed op uzelf op de eenzame Andorvlakte."
						}
					},
					"WomanAtTable": {
						"Line01": "Vrouw aan Tafel: Je ziet eruit als een eerlijke meid.",
						"Line02": "Kom even zitten.",
						"Line03": "Er wordt gezegd dat degenen die diep de aarde ingaan",
						"Line04": "leren om de zonden van hebzucht te overwinnen.",
						"Line05": "Dat zou waar kunnen zijn, maar ik weet wel dit:",
						"Line06": "een kaars en een zilveren sleutel kunnen je geluk brengen.",
						"Line07": "Schiet op, meisje, de tijd dringt!"
					},
					"DefeatedSoldier": {
						"Line01": "Verslagen Soldaat: Ik ben zo uitgeput van mijn reizen.",
						"Line02": "Had ik maar geweten dat ik de kloof met een ladder kon oversteken,",
						"Line03": "dat had me maanden bespaard!"
					},
					"KrebbTheWeary": {
						"Line01": "Krebb de Vermoeide: Waarom een heel leven lang verslagen zijn zoals ik?",
						"Line02": "Maak er een einde aan. Stop, ga naar huis, geef het op.",
						"Line03": "Je bent geduldig. Ik zie dat je je niet zomaar overgeeft.",
						"Line04": "Oké, neem deze dolk van me af.",
						"Line05": "Die zal je vast meer helpen dan het mij ooit heeft gedaan."
					}
				}
			},
			"ForestOfOgham": {
				"OghamMerchant": {
					"PlentyToBuy": {
						"Line01": "Koopman: Kom eens hier, slimme jongedame.",
						"Line02": "Ik heb een geweldig koopje voor je!",
						"Line03": "Neem gratis wat Ogham Kruid en kijk dan even rond.",
						"Line04": "Er is genoeg te koop!"
					},
					"NothingToSell": {
						"Line01": "Koopman: Het spijt me, Prinses, ik heb vandaag niets te koop!"
					},
					"WisePurchase": {
						"Line01": "Koopman: Dat was een wijze aankoop, jonge prinses!"
					}
				},
				"Enid": {
					"ComeClose": {
						"Line01": "Enid: Kom dichterbij, Prinses! Dan vertel ik u een groot geheim!"
					},
					"TrustNoOneWithHair": {
						"Line01": "Enid: Hoor wat ik fluister: Vertrouw niemand met haar! Huh-huh-huh!",
						"Line02": "Vertrouw zelfs mij niet! Ha-haa!",
						"Line03": "Ik heb een van uw levens gestolen! Hi-hi-hi-hi-hi!"
					},
					"TooClever": {
						"Line01": "Enid: Aaaaaaah, u bent te slim voor Enid!"
					},
					"GoAway": {
						"Line01": "Enid: Ga weg! Ik heb niets voor u.",
						"Line02": "Ik heb u niets te zeggen."
					}
				}
			},
			"ShrineOfEarth": {
				"Shurmak": {
					"EvilsOfGreed": {
						"Line01": "Shurmak: Pas op voor de kwaden van hebzucht.",
						"Line02": "Handel snel, denk goed na,",
						"Line03": "en je zult beloond worden."
					}
				},
				"Llort": {
					"YouAreNotWanted": {
						"Line01": "Llort: Kom hier niet naar binnen! Je bent niet welkom.",
						"Line02": "Ik ben Llort, en dit is mijn tempel! Helemaal van mij!"
					},
					"ShrineOfRock": {
						"Line01": "Llort: Ik zei dit is mijn grot! Van mij!",
						"Line02": "Dit is de Steentempel en ik ben Llort!",
						"Line03": "Ga weg! Neem niks mee! Het is allemaal van mij!"
					},
					"TakingMyThings": {
						"Line01": "Llort: Je hebt mijn dingen gejat.",
						"Line02": "Leg ze terug! Raak niets meer aan!",
						"Line03": "Dat zijn mijn juwelen!"
					}
				},
				"RedSardak": {
					"NoThreat": {
						"Line01": "Rode Sardak: Tralalie, tralala!",
						"Line02": "Je bent geen bedreiging voor ons hier beneden!"
					},
					"FailedMission": {
						"Line01": "Rode Sardak: Ik heb gefaald in mijn missie!",
						"Line02": "Llort, deze vijand komt eraan!"
					}
				},
				"BlueSardak": {
					"ThoughtIWasAlone": {
						"Line01": "Blauwe Sardak: Huh? Ik... Ik dacht dat ik alleen was hier!",
						"Line02": "Llort, er zit een vijand in je grot!"
					},
					"CouldntStopHer": {
						"Line01": "Blauwe Sardak: Llort, ik kon haar niet stoppen!",
						"Line02": "Bereid je voor op een gevecht!"
					}
				},
				"YellowSardak": {
					"EnemyOfMyBoss": {
						"Line01": "Gele Sardak: Hé! Wie is daar?",
						"Line02": "Ben jij niet een vijand van mijn baas Llort?",
						"Line03": "Je mag hier niet naar binnen."
					},
					"ShesComing": {
						"Line01": "Gele Sardak: Hé, Llort! Kijk uit!",
						"Line02": "Ze komt eraan!"
					}
				},
				"TreasureChest": {
					"WhatDoWeHaveHere": {
						"Line01": "Schatkist: Oh-ho-ho! Oh, wat hebben we hier?!",
						"Line02": "Daar komt een menselijk persoon aan!",
						"Line03": "Nou, wat een geweldige gebeurtenis! Hoe-hoe!"
					}
				}
			},
			"ForestOfFindo": {
				"TalkingToadstool": {
					"Line01": "Paddenstoel: Hoehoehoehoe!",
					"Line02": "Die Mobilins zijn zo traag en dom...",
					"Line03": "dat ze nog steeds hun soep met een vork proberen te eten!",
					"Line04": "Huh-huh-huh-huh-huh-huh!"
				}
			},
			"MeadowOfSkotness": {
				"Farmer": {
					"HelloThereMiss": {
						"Line01": "Boer: Hallo daar, jongedame!",
						"Line02": "Pas op als je de weiden in gaat!",
						"Line03": "Ze zitten vol met Leevers en Tektites rond deze tijd van het jaar!"
					},
					"WhatBringsYouThisWay": {
						"Line01": "Boer: Wat leuk om je te zien!",
						"Line02": "Van waar ben je hier gekomen?",
						"Line03": "Sorry, maar ik moet weer verder met ploegen."
					}
				}
			},
			"GreatWimbich": {
				"TownMerchant": {
					"ForThirtySuns": {					
						"Line01": "Dorpshandelaar: Dertig zonnen lang hebben mijn man en ik",
						"Line02": "de grote slotenmakersmederij voor Warbane gerund.",
						"Line03": "Toen kwam Gannon en dwong Warbane om voor zijn",
						"Line04": "eigen kwaadaardige plannen te werken.",
						"Line05": "De smederij werd gesloten.",
						"Line06": "Warbane bouwde zijn eigen versterkte toevluchtsoord",
						"Line07": "ergens in het Bos van Canvula in het noorden.",
						"Line08": "We hadden niets meer behalve deze snuisterijen om te verkopen!",
						"Line09": "Misschien heb je iets aan ze in de ondergrondse wereld.",
						"Line10": "Ik kan ze niet goedkoop afstaan."
					},
					"KindaTiredToday": {					
						"Line01": "Dorpshandelaar: Ik ben een beetje... moe vandaag.",
						"Line02": "Alles verandert in... modder."
					}
				},
				"Yvonne": {
					"GiveFlute": {
						"Line01": "Yvonne: Hallo, Zelda. Mijn naam is Yvonne.",
						"Line02": "Je ziet er uit als een mede-muziekliefhebber.",
						"Line03": "Muziek kan zelfs een woest beest kalmeren.",
						"Line04": "Ik heb vernomen dat Shurmak je met haar vriendschap heeft vereerd.",
						"Line05": "Ze heeft mij ooit geholpen, dus nu zal ik ook jou helpen.",
						"Line06": "Luister hier eens naar!",
						"Line07": "*Yvonne bespeelt de fluit*",
						"Line08": "Hihi! De fluit kan worden gebruikt om het woeste gesis van een slang te kalmeren."
					},
					"Singing": {
						"Line01": "*Yvonne zingt*"
					}
				},
				"EricAndIan": {
					"MeetTheTwins": {
						"Line01": "Tweeling (Eric & Ian): Hallo, Zelda. Ik ben Eric en dit is Ian.",
						"Line02": "Wat bedoel je? Ik ben Eric, dat ben ik altijd geweest.",
						"Line03": "Nee, dat ben je niet, ik ben Eric!"
					},
					"MeetOurFather": {
						"Line01": "Ach, het doet er niet toe wie wij zijn.",
						"Line02": "Je zou het gele huis eens moeten bezoeken en onze vader ontmoeten!",
						"Line03": "Hij zal je iets interessants vertellen."
					}
				},
				"Alice": {
					"MyGoodness": {
						"Line01": "Alice: Mijn hemel, het is de dappere Zelda!",
						"Line02": "Ik heb van iedereen zoveel over je gehoord.",
						"Line03": "Kom lekker bij Alice zitten en doe een oude vrouw een plezier."
					},
					"PeculiarLittleTown": {
						"Line01": "Alice: Great Wimbich is zo'n eigenaardig dorpje.",
						"Line02": "De winkels in de Bovenstraat hebben alles wat je maar kunt wensen",
						"Line03": "en als je het daar niet kunt vinden,",
						"Line04": "kom je het vast en zeker tegen op het marktplein."
					},
					"IfYaLikeMusic": {
						"Line01": "Alice: Maar als je van muziek houdt, volg dan je oren naar Yvonne,",
						"Line02": "de muzikante hier verderop de Skotnessweg.",
						"Line03": "Als je tijdens je reizen uitgeput raakt, is dit een geweldige plek om bij te komen."
					},
					"NothingMoreToSay": {
						"Line01": "Alice: Zelda, het was leuk om met je te praten,",
						"Line02": "maar ik heb je niets meer te vertellen.",
						"Line03": "Kom nog eens langs, oké?"
					},
					"RealNice": {
						"Line01": "Alice: Ja, Zelda, het is echt heel mooi in deze omgeving."
					}
				},
				"GhostFarmer": {
					"StayBack": {
						"Line01": "Spookboer: Blijf daar!",
						"Line02": "Deze schitterende boerderij is helemaal van mij.",
						"Line03": "Is het niet prachtig?",
						"Line04": "Ik bewerk dit land al driehonderd jaar."
					},
					"StillHere": {
						"Line01": "Nee? Ben je er nog steeds?",
						"Line02": "Oké, ik geef het toe, mijn boerderij is een puinhoop.",
						"Line03": "Ganon en zijn wrede vrienden hebben al mijn gewassen en gereedschap meegenomen.",
						"Line04": "Je moet ons redden!",
						"Line05": "Kom dichterbij, dan vertel ik je een geheim."
					},
					"ShrineOfWater_Classic": {
						"Line01": "Spookknecht: Hier ver vandaan leeft een bondgenoot van Ganon in een wereld van water.",
						"Line02": "De ingang is verborgen en je zult hem nooit vinden",
						"Line03": "als je bang bent voor stromend water.",
						"Line04": "Met de rode laarzen kun je elke fjord in de Skotnessrivier oversteken."
					},
					"ShrineOfWater_Remastered": {
						"Line01": "Spookboer: Hier ver vandaan leeft een bondgenoot van Ganon in een wereld van water.",
						"Line02": "De ingang is verborgen en je zult hem nooit vinden",
						"Line03": "als je bang bent voor stromend water."
					}
				},
				"TwinFather": {
					"WelcomeToMyHome": {
						"Line01": "Vader van de Tweeling: Hallo, Zelda! Welkom in mijn huis.",
						"Line02": "Ik ben de vader van de tweeling, en zelfs ik weet niet welke tweeling welke is.",
						"Line03": "Maar het is leuk dat je langskomt.",
						"Line04": "Mijn naam is Zelda en jij bent vast de vader.",
						"Line05": "Nee, wacht! Dat kan niet kloppen, je bent net binnengekomen.",
						"Line06": "Jij bent vast Zelda!"
					},
					"CoalAndRubies": {
						"Line01": "Ik weet dit zeker, want ik was vroeger",
						"Line02": "mijnwerker op de Vlakte van Hobel:",
						"Line03": "Steenkool en Robijnen zijn dezelfde substantie.",
						"Line04": "Ik geloof dat er ergens in de buurt een breker is.",
						"Line05": "Eh, ga in vrede, kleintje. Eh... ben jij Eric of Ian?"
					},
					"VaguelyFamiliar": {
						"Line01": "Vader van de Tweeling: Je... komt me vaag bekend voor, maar eh...",
						"Line02": "het is echt tijd voor mijn dutje.",
						"Line03": "Ben jij Eric of Ian?"
					}
				},
				"Lounger": {
					"HiThere": {
						"Line01": "Lounger: Hi there! Don't worry about the dog, he won't bite.",
						"Line02": "I wish he did bite, then there'd be a little excitement here in Great Wimbich.",
						"Line03": "Nothing ever happens here!",
						"Line04": "When people do come, they're even more boring.",
						"Line05": "Everybody's dull."
					},
					"DontWannaSeeYou": {
						"Line01": "Lounger: Ugh... I don't want to see you again.",
					},
					"GoAway": {
						"Line01": "Lounger: Why don't you go away and be boring someplace else?",
						"Line02": "Good riddance."
					}
				},
				"GeneralShopkeeper": {
					"HelloLittleOne": {
						"Line01": "Shopkeeper: Hello, little one! Heh!",
						"Line02": "I hope I can be of help to you.",
						"Line03": "Did you see my dog out on Upper Street?",
						"Line04": "How did he smell?",
						"Line05": "Knave: With his nose, of course!",
						"Line06": "*laughter from both men*"
					},
					"GoodsOnTheTable": {
						"Line01": "Shopkeeper: You see my goods on the table?",
						"Line02": "You may purchase whatever you wish.",
						"Line03": "There's even a bone here that my dog would love!"
					},
					"HowAreYou": {
						"Line01": "Shopkeeper: Ooooooh! Heh!",
						"Line02": "How are you? Ha ha ha!",
						"Line03": "May I be of some service?"
					},
					"ThatWasSmart": {
						"Line01": "Shopkeeper: Oooooo! *laughter*",
						"Line02": "That was smart!",
						"Line03": "Come again, little one! Come again."
					}
				},
				"Knave": {
					"ShrineTakenLightly": {
						"Line01": "Knave: Is a shrine a thing to be taken lightly? *laughter*",
						"Line02": "No, and a candle is needed to light a shrine! *laughter*"
					},
					"JokeToPassTime": {
						"Line01": "Knave: *nervous laughter* Oh-hoh-hooo!",
						"Line02": "Oooo, we may joke to pass the time, princess, but...",
						"Line03": "we are all troubled about the future of Tolemac.",
						"Line04": "You must seek out the tablets and save us! *nervous laughter*"
					},
					"WatchOutForGanon": {
						"Line01": "Knave: *laughing* Oo-hooo!",
						"Line02": "Watch out for that Ganon, Zelda!",
						"Line03": "Ooooooooo!"
					}
				},
				"MadameKriggle": {
					"ApproachedAtLast": {
						"Line01": "Madame Kriggle: At last!",
						"Line02": "Madame Kriggle is approached by an innocent",
						"Line03": "who wishes to grow in the use of magic and spells.",
						"Line04": "My old friend Shurmak has told me of your coming;",
						"Line05": "I have awaited you many moons and suns.",
						"Line06": "It is important to remember",
						"Line07": "that magic adornments will defend thee from many beings",
						"Line08": "who will attack you with magic.",
						"Line09": "You may purchase my Turquoise Ring",
						"Line10": "or my Jade Amulet to raise your defenses."
					},
					"TouchWhatYouWantToUse": {
						"Line01": "Madame Kriggle: Touch what you wish to use,",
						"Line02": "and leave me your Rubies.",
						"Line03": "I promise you will be pleased."
					},
					"WonderfulToSeeYou": {
						"Line01": "Madame Kriggle: How wonderful to see you, my dear!"
					},
					"WiseChoice": {
						"Line01": "Madame Kriggle: A wise choice for setting out! Oh-hoh...",
						"Line02": "Come and see me again!"
					}
				},
				"Smithy": {
					"WhoGoesThere": {
						"Line01": "Smithy: Who goes there!? Oh, it's a young girl.",
						"Line02": "Don't touch anything!",
						"Line03": "People steal me blind; I know you all hate me!",
						"Line04": "What did I ever do to any of you!?",
						"Line05": "I give an honest bargain, I grind your coal into Rubies,",
						"Line06": "I stay open late and work my fingers to the bone.",
						"Line07": "Do any of you appreciate it? No!"
					},
					"ChooseItemAndHurry": {
						"Line01": "Smithy: Choose your item and hurry, the world is full of thieves.",
					},
					"CongratsOnFindingCoal": {
						"Line01": "Smithy: Congratulations on finding such a fine piece of coal!",
						"Line02": "After Wimbich taxes - my commission, of course...",
						"Line03": "*cough* that leaves, umm..."
					}
				}
			},
			"PlainOfDonora": {
				"YalzanTheStern": {
					"Toll10Rubies": {
						"Line01": "Yalzan de Strenge: Voor tien Robijnen mag je door de poort, jongedame, niet minder."
					},
					"Toll20Rubies": {
						"Line01": "Yalzan de Strenge: Oh, dwing je me je door te laten?",
						"Line02": "De prijs is nu twintig Robijnen."
					}
				},
				"KronThePegleg":{
					"EverybodyComesTo": {
						"Line01": "Kron de Eenbeen: Iedereen komt naar Kron de Eenbeen. Gezellig!"
					},
					"FineItemForSale": {
						"Line01": "Nou, hier heb ik toch een mooi voorwerp te koop!"
					}
				}
			}
		}
		
	}
